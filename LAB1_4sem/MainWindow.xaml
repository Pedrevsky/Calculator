<Window x:Class="LAB1_4sem.MainWindow"        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        xmlns:local="clr-namespace:LAB1_4sem"        mc:Ignorable="d"        ResizeMode="NoResize"        Title="Calculator" Height="600" Width="450">    <Grid>        <Grid.ColumnDefinitions>            <ColumnDefinition Width="2*"/>            <ColumnDefinition Width="2*"/>            <ColumnDefinition Width="2*"/>            <ColumnDefinition Width="2*"/>        </Grid.ColumnDefinitions>        <Grid.RowDefinitions>            <RowDefinition Height="1*"/>            <RowDefinition Height="1*"/>            <RowDefinition Height="1*"/>            <RowDefinition Height="1*"/>            <RowDefinition Height="1*"/>        </Grid.RowDefinitions>                <TextBox Name="text_box" SpellCheck.IsEnabled="True" IsEnabled="False"                 Text=""                 TextAlignment="Center"                 TextWrapping="Wrap"                 Grid.ColumnSpan="3"                 FontSize="30"                 Width="317"                 Height="52"                 HorizontalAlignment="Center"                 VerticalAlignment="Center"/>        <Button x:Name="rowna_sie"                Grid.Row="0"                Grid.Column="3"                                Content="="                Click="button_rowna_sie"/>        <Button x:Name="plus"                Grid.Row="3"                Grid.Column="3"                Content="+"                Click="button_dzialanie"/>        <Button x:Name="minus"                Grid.Row="2"                Grid.Column="3"                Content="-"                Click="button_dzialanie"/>        <Button x:Name="mnozenie"                Grid.Row="1"                Grid.Column="3"                Content="*"                Click="button_dzialanie"/>        <Button x:Name="dzielenie"                Grid.Row="4"                Grid.Column="3"                Content="/"                Click="button_dzialanie"/>        <Button x:Name="click_0"                Grid.Row="4"                Grid.Column="1"                Content="0"                Click="digit_button_click">        </Button>        <Button x:Name="click_1"                Grid.Row="3"                Grid.Column="0"                Content="1"                Click="digit_button_click">        </Button>        <Button x:Name="click_2"                Grid.Row="3"                Grid.Column="1"                Content="2"                Click="digit_button_click">        </Button>        <Button x:Name="click_3"                Grid.Row="3"                Grid.Column="2"                Content="3"                Click="digit_button_click">        </Button>        <Button x:Name="click_4"                Grid.Row="2"                Grid.Column="0"                Content="4"                Click="digit_button_click">        </Button>        <Button x:Name="click_5"                Grid.Row="2"                Grid.Column="1"                Content="5"                Click="digit_button_click">        </Button>        <Button x:Name="click_6"                Grid.Row="2"                Grid.Column="2"                Content="6"                Click="digit_button_click">        </Button>        <Button x:Name="click_7"                Grid.Row="1"                Grid.Column="0"                Content="7"                Click="digit_button_click">        </Button>        <Button x:Name="click_8"                Grid.Row="1"                Grid.Column="1"                Content="8"                Click="digit_button_click">        </Button>        <Button x:Name="click_9"                Grid.Row="1"                Grid.Column="2"                Content="9"                Click="digit_button_click">        </Button>        <Button x:Name="click_dot"                Grid.Row="4"                Grid.Column="2"                Content=","                Click="button_dot"/>        <Button x:Name="clear"                Grid.Row="4"                Grid.Column="0"                Content="C"                Click="clear_click"/>    </Grid></Window>